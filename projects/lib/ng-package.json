{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/lib",
  "lib": {
    "flatModuleFile": "lib",
    "entryFile": "src/public-api.ts",
    "styleIncludePaths": ["./src/assets/styles"],
    "umdModuleIds": {
      "@ng-bootstrap/ng-bootstrap": "@ng-bootstrap/ng-bootstrap"
    }
  },
  "assets": [
    "./assets/**/*.scss",
    "./assets/fonts/*.*"
  ]
}
